{
  "manifest_version": 2,
  "name": "meetical",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
	"48": "icons/meetical-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.calendar.google.com/*"],
      "js": ["gCalendar.js"]
    }
  ]
}