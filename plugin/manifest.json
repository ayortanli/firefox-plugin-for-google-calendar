{
  "manifest_version": 2,
  "name": "meetical",
  "version": "1.0",

  "description": "Adds a custom section to Google Calendar's Schedule Meeting view",

  "icons": {
	"48": "icons/meetical-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.calendar.google.com/*"],
      "js": ["main.js"]
    }
  ]
}